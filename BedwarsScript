AntiKnockback.Name = "AntiKnockback"
AntiKnockback.Parent = Bedwars
AntiKnockback.BackgroundColor3 = Color3.fromRGB(16, 17, 16)
AntiKnockback.BorderColor3 = Color3.fromRGB(24, 24, 24)
AntiKnockback.Position = UDim2.new(0.105975308, 0, 0.145030022, 0)
AntiKnockback.Size = UDim2.new(0, 306, 0, 43)
AntiKnockback.Font = Enum.Font.Highway
AntiKnockback.Text = "AntiKnockback"
AntiKnockback.TextColor3 = Color3.fromRGB(255, 255, 255)
AntiKnockback.TextSize = 25.000
AntiKnockback.MouseButton1Click:Connect(function()
local s = require(game:GetService("ReplicatedStorage").TS.damage["knockback-util"]).KnockbackUtil
s.applyKnockbackDirection = function(...)end
s.applyKnockback = function(...)end
end)

KillAura.Name = "KillAura"
KillAura.Parent = Bedwars
KillAura.BackgroundColor3 = Color3.fromRGB(16, 17, 16)
KillAura.BorderColor3 = Color3.fromRGB(24, 24, 24)
KillAura.Position = UDim2.new(0.105975308, 0, 0.383559048, 0)
KillAura.Size = UDim2.new(0, 306, 0, 47)
KillAura.Font = Enum.Font.Highway
KillAura.Text = "KillAura"
KillAura.TextColor3 = Color3.fromRGB(255, 255, 255)
KillAura.TextSize = 25.000
KillAura.MouseButton1Click:Connect(function()
	if KillAura then
		KillAura = false
		KillAura.Text = "Killaura"
	else
		Killaura = true
		KillAura.Text = "Killaura (Toggled)"
		repeat
			wait(.1)
			for i,v in pairs(game.Players:GetPlayers()) do
				local vchr = v.Character
				if vchr and v~=Player and v.Team~=Player.Team and Character and Character.Humanoid.Health>0 then
					local vhum = vchr:FindFirstChild("Humanoid")
					local vroot = vchr:FindFirstChild("HumanoidRootPart")
					if vhum and vroot and vhum.Health>0 and (vroot.Position-Character.HumanoidRootPart.Position).magnitude<=Range then
						RemoteFolder.SwordHit:InvokeServer({entityInstance=vchr,weapon=Character.HandInvItem.Value})
					end
				end
			end
		until not KillAura
	end
end)

