BuyWool.Name = "BuyWool"
BuyWool.Parent = Bedwars
BuyWool.BackgroundColor3 = Color3.fromRGB(16, 17, 16)
BuyWool.BorderColor3 = Color3.fromRGB(24, 24, 24)
BuyWool.Position = UDim2.new(0.105975308, 0, 0.637340903, 0)
BuyWool.Size = UDim2.new(0, 306, 0, 47)
BuyWool.Font = Enum.Font.Highway
BuyWool.Text = "BuyWool"
BuyWool.TextColor3 = Color3.fromRGB(255, 255, 255)
BuyWool.TextSize = 25.000
BuyWool.MouseButton1Click:Connect(function()
	if Character and Character.Humanoid.Health>0 then
		RemoteFolder.BedwarsPurchaseItem:InvokeServer({shopItem={price=8,currency="iron",itemType="wool_white",amount=16}})
	end
end)

InstantPickup.Name = "InstantPickup"
InstantPickup.Parent = Bedwars
InstantPickup.BackgroundColor3 = Color3.fromRGB(16, 17, 16)
InstantPickup.BorderColor3 = Color3.fromRGB(24, 24, 24)
InstantPickup.Position = UDim2.new(0.105975308, 0, 0.763832569, 0)
InstantPickup.Size = UDim2.new(0, 306, 0, 51)
InstantPickup.Font = Enum.Font.Highway
InstantPickup.Text = "InstantPickup"
InstantPickup.TextColor3 = Color3.fromRGB(255, 255, 255)
InstantPickup.TextSize = 25.000
InstantPickup.MouseButton1Click:Connect(function()
	if InstantPickActive then
		InstantPickActive = false
		InstantPickButton.Text = "Instant Pickup"
	else
		InstantPickActive = true
		InstantPickButton.Text = "Instant Pickup (Toggled)"
		repeat
			wait()
			for i,v in pairs(workspace.ItemDrops:GetChildren()) do
				if Character and Character.Humanoid.Health>0 and (v.Position-Character.HumanoidRootPart.Position).magnitude<=10 then
					local x,y,z = math.ceil(v.Position.X/3),math.ceil(v.Position.Y/3),math.ceil(v.Position.Z/3)
					coroutine.wrap(function()
						RemoteFolder.PickupItemDrop:InvokeServer({itemDrop=v})
					end)()
				end
			end
		until not InstantPickActive
	end
end)

-- Scripts:

local function UIIZHK_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local bg = script.Parent.Parent.Frame
	
	script.Parent.MouseButton1Click:Connect(function()
	
		
		if bg:TweenPosition(UDim2.new(0.34, 0,0.247, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quint, 2) then
			bg:TweenPosition(UDim2.new(0.542, 0, -0.433, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quint, 2)
		else
			if	bg:TweenPosition(UDim2.new(0.542, 0, -0.433, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quint, 2) then
				bg:TweenPosition(UDim2.new(0.34, 0,0.247, 0), Enum.EasingDirection.In, Enum.EasingStyle.Quint, 2)
			end
			end
	end)
end
coroutine.wrap(UIIZHK_fake_script)()
local function ZLFLD_fake_script() -- ScreenGui.Script 
	local script = Instance.new('Script', ScreenGui)

	print("Hello world!")
	
end
coroutine.wrap(ZLFLD_fake_script)()
